programa {
  funcao inicio() {
   
   //Declaração das variáveis
    inteiro idade
 //Voto Obrigatório

    escreva("digite sua idade: ")
    leia(idade)

    
    //Estrutura de decisão
    se(idade>=18 e idade<65 ){
      escreva("\n Voto obrigatório")
      } senao se (idade>=16 e idade<18 ou idade>=65){
      escreva("\n Voto opicional")
      }  
      senao{
        escreva("\n Voto não Permitido")
      }
      
      
     
    

    
    
  }
}
